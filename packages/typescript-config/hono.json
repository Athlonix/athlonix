{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "hono",
  "extends": "./base.json",
  "compilerOptions": {
    "outDir": "dist",
    "types": ["node", "vitest/globals"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true
  }
}
